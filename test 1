#include <iostream>
#include <stdio.h>
#include <fstream>
#include <sstream>

#include <vector>
#include <string>

int count_digit(int number) {
    int count = 0;
    while (number != 0) {
        number = number / 10;
        count++;
    }
    return count;
}

int square_digits(int num) {
    std::vector <int> myv;
     std::stringstream ss1,exit;
    ss1 << num;
    std::string s, to_ret;
    ss1 >> s;
    for (char c : s)
    {
        myv.push_back(c - '0');
    }
    for (int i{ 0 }; i< myv.size(); i++)
    {
        myv[i] = (myv[i] * myv[i]);
    }
    for (int i : myv)
    {
        exit << i;
    }
    exit >> to_ret;
    num = std::stoi(to_ret);


    return num;
}


int main()
{
    int num;
    std::cin >> num;
    std::cout << square_digits(num);
    



}
